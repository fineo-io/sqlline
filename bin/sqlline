#!/bin/bash
# sqlline - Script to launch SQL shell on Unix, Linux or Mac OS

DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
DEBUG=${DEBUG_OPTS:-"-agentlib:jdwp=transport=dt_socket,server=y,suspend=n,address=5005"}
OPTS=${SQL_LINE_SYS_OPTIONS:-"-Dsqlline.Isolation=TRANSACTION_NONE"}
exec java -cp "$DIR/lib/*" $OPTS $DEBUG sqlline.SqlLine "$@"

# End sqlline
